\__ProgramNode [<program>]

	\__LetNode [<expression>]
		\__AssignNode:
			\__IdentifierNode: x

		\__BlockNode [<expression>]
			\__IfNode  [<expression>]
				\_ Condition

				\_ Body
					\__BlockNode [<expression>]
						\__PrintNode [<sentence>]
							\__PlusNode [<expression>]
								\__IdentifierNode: x

				\_ Else Body
					\__BlockNode [<expression>]
						\__PrintNode [<sentence>]
							\__IdentifierNode: x